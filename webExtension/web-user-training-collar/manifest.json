{
  "description": "Web User Training Collar",
  "manifest_version": 2,
  "name": "web-user-training-collar",
  "version": "1.1",
  "content_scripts": [
    {
      "matches": ["<all_urls>"],
      "js": ["content-script.js"]
    }
  ],
   "permissions": [
    "webRequest"
  ],
  "applications": {
    "gecko": {
      "id": "web-user-training-collar@jaspervanloenen.com",
      "strict_min_version": "42.0",
      "strict_max_version": "50.*"
    }
  }
}